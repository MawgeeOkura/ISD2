-- create datebase

CREATE DATABASE OMOA_DB;

use OMOA_DB

-- create table statements

-- 1
CREATE TABLE Role (
	id BIGINT NOT NULL AUTO_INCREMENT,
	permission VARCHAR(20),
	status VARCHAR(20),
	updateDate DATETIME,

	PRIMARY KEY (id)
);

-- 2
CREATE TABLE Staff (
	id BIGINT NOT NULL AUTO_INCREMENT,
	firstName VARCHAR(40),
	lastName VARCHAR(40),
	password VARCHAR(255),
	email VARCHAR(60),
	phone VARCHAR(12),
	createDate DATETIME,
	roleId BIGINT NOT NULL,

	PRIMARY KEY (id)

);
ALTER TABLE Staff ADD CONSTRAINT FK_role_Id FOREIGN KEY (roleId) REFERENCES Role (id);

-- 3
CREATE TABLE PaymentDetails (
	id BIGINT NOT NULL AUTO_INCREMENT,
	cardNumber VARCHAR(255),
	cvc INTEGER(4),
	expiryMonth VARCHAR(3),
	expiryYear INTEGER(4),
	address1 VARCHAR(255),
	address2 VARCHAR(255),
	state VARCHAR(3),
	postcode INTEGER(4),
	
	PRIMARY KEY (id)
);

-- 4
CREATE TABLE Customer (
	id BIGINT NOT NULL AUTO_INCREMENT,
	firstName VARCHAR(40),
	lastName VARCHAR(40),
	password VARCHAR(255),
	email VARCHAR(60),
	phone VARCHAR(12),
	createDate DATETIME,
	paymentDetailsId BIGINT,

	PRIMARY KEY (id)
);
ALTER TABLE Customer ADD CONSTRAINT FK_paymentDetails_Id FOREIGN KEY (paymentDetailsId) REFERENCES PaymentDetails (id);

-- 5
CREATE TABLE User (
	id BIGINT NOT NULL AUTO_INCREMENT,
	customerId BIGINT,
	staffId BIGINT,
	
	PRIMARY KEY (id)
);
ALTER TABLE User ADD CONSTRAINT FK_staff_Id FOREIGN KEY (staffId) REFERENCES Staff (id);
ALTER TABLE User ADD CONSTRAINT FK_customer_Id FOREIGN KEY (customerId) REFERENCES Customer (id);

-- 8
CREATE TABLE Movie (
	id BIGINT NOT NULL AUTO_INCREMENT,
	name VARCHAR(60),
	description TEXT,
	genre TEXT,
	releaseDate DATETIME,
	runtime INTEGER,
	keywords LONGTEXT,
	rating DECIMAL(2,1),
	price DECIMAL(3,2),
	status VARCHAR(20),
	
	PRIMARY KEY (id)
);

-- 6
CREATE TABLE OrderDetails (
	id BIGINT NOT NULL AUTO_INCREMENT,
	movie_1_Id BIGINT NOT NULL,
	movie_2_Id BIGINT,
	movie_3_Id BIGINT,
	movie_4_Id BIGINT,
	movie_5_Id BIGINT,
	
	PRIMARY KEY (id)
);
ALTER TABLE OrderDetails ADD CONSTRAINT FK_movie_1_Id FOREIGN KEY (movie_1_Id) REFERENCES Movie (id);
ALTER TABLE OrderDetails ADD CONSTRAINT FK_movie_2_Id FOREIGN KEY (movie_2_Id) REFERENCES Movie (id);
ALTER TABLE OrderDetails ADD CONSTRAINT FK_movie_3_Id FOREIGN KEY (movie_3_Id) REFERENCES Movie (id);
ALTER TABLE OrderDetails ADD CONSTRAINT FK_movie_4_Id FOREIGN KEY (movie_4_Id) REFERENCES Movie (id);
ALTER TABLE OrderDetails ADD CONSTRAINT FK_movie_5_Id FOREIGN KEY (movie_5_Id) REFERENCES Movie (id);

-- 7
CREATE TABLE Orders (
	id BIGINT NOT NULL AUTO_INCREMENT,
	orderDate DATETIME,
	userId BIGINT NOT NULL,
	orderDetailsId BIGINT NOT NULL,
	paymentDetailsId BIGINT NOT NULL,

	PRIMARY KEY (id)
);

ALTER TABLE Orders ADD CONSTRAINT FK_orderPaymentDetails_Id FOREIGN KEY (paymentDetailsId) REFERENCES PaymentDetails (id);
ALTER TABLE Orders ADD CONSTRAINT FK_orderDetails_Id FOREIGN KEY (orderDetailsId) REFERENCES OrderDetails (id);
ALTER TABLE Orders ADD CONSTRAINT FK_orderUser_Id FOREIGN KEY (userId) REFERENCES User (id);



-- 9
CREATE TABLE AdminLog (
	id BIGINT NOT NULL AUTO_INCREMENT,
	ipAddress VARCHAR(30),
	location VARCHAR(255),
	sessionStatus VARCHAR(20),
	eventDate DATETIME,
	userId BIGINT NOT NULL,
	
	PRIMARY KEY (id)
);
ALTER TABLE AdminLog ADD CONSTRAINT FK_user_Id FOREIGN KEY (userId) REFERENCES User (id);

